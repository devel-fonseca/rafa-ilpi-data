    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.22.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "bullmq": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "csv-parse": "^6.1.0",
    "date-fns": "^3.2.0",
    "date-fns-tz": "^3.2.0",
    "easy-email-core": "^4.16.4",
    "helmet": "^7.1.0",
